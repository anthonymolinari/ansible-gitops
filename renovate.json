{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
        "matchUpdateTypes": ["major", "minor", "patch", "pin", "digest"],
        "automerge": true,
    },
  ],
  "timezone": "America/Los_Angeles",
  "ignoreTests": true,
}
