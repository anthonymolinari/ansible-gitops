$TTL 2d

$ORIGIN homelab.lan.

@       IN      SOA     ns.homelab.lan. info.homelab.lan. (
                        20230925   ; serial
                        12h        ; refresh
                        15m        ; retry
                        3w         ; expire
                        2h         ; minimum ttl
)


        IN      NS      ns.homelab.lan.

ns      IN      A       10.32.1.7

